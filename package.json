{
  "name": "canismajor",
  "version": "0.0.1",
  "scripts": {
    "start": "nodemon --exec babel-node src/bin/www --verbose",
    "build": "rm -rf dist && babel src --out-dir dist && babel src/bin/www --out-dir dist/ && mv dist/src/bin dist/bin && rm -rf dist/src && mkdir dist/config && cp src/config/config.json dist/config/config.json",
    "serve": "node dist/bin/www.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha tests --timeout 15000 --recursive --compilers js:babel-core/register"
  },
  "dependencies": {
    "app-root-path": "^2.0.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "moment": "^2.18.1",
    "nodemon": "^1.19.4",
    "uuid": "^8.2.0",
    "web3": "^1.2.11",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-env": "^1.5.2"
  }
}
