{
  "name": "canismajor",
  "version": "0.0.1",
  "scripts": {
    "start": "nodemon --exec babel-node src/bin/www --verbose",
    "build": "rm -rf dist && babel src --out-dir dist && babel src/bin/www --out-dir dist/ && mv dist/src/bin dist/bin && rm -rf dist/src",
    "serve": "node dist/bin/www.js",
    "test": "mocha --require babel-core/register --require babel-polyfill --recursive",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "ethereumjs-tx": "^2.1.2",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "node-fetch": "^2.6.1",
    "nodemon": "^1.19.4",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.1",
    "web3": "^1.3.0",
    "xmlhttprequest": "1.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "mocha": "^8.2.1",
    "chai": "^4.2.0"
  }
}
