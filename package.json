{
  "name": "canismajor",
  "version": "0.0.1",
  "scripts": {
    "start": "nodemon --exec babel-node src/bin/www --verbose",
    "build": "rm -rf dist && babel src --out-dir dist && babel src/bin/www --out-dir dist/ && mv dist/src/bin dist/bin && rm -rf dist/src && mkdir dist/config && cp src/config/config.json dist/config/config.json",
    "serve": "node dist/bin/www.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha tests --timeout 15000 --recursive --compilers js:babel-core/register",
    "migrate": "./node_modules/.bin/sequelize db:migrate",
    "seed": "./node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "app-root-path": "^2.2.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.4",
    "express": "^4.17.1",
    "gulp": "^4.0.2",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "nodemon": "^1.19.4",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "uuid": "^8.3.1",
    "web3": "^1.3.0",
    "xmlhttprequest": "1.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-env": "^1.5.2"
  }
}
